@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    var content = (ContentModels.OurServices)Model.Content;
}


<div class="card">
    <div class="card-body">
        <hr>
        <h5> class="card-title"@content.Value("ourServiceTitle")</h5>
        <p>@content.Value("ourServiceDescription")</p>
        <a href="@content.Value("ourServiceLink")" ><i class="fa-light fa-arrow-right"></i></a>
    </div>
</div>